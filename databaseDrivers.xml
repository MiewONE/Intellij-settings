<application>
  <component name="LocalDatabaseDriverManager" version="201">
    <driver id="h2.unified">
      <artifact id="H2" name="H2" use="true" version="1.4.200" />
    </driver>
    <driver id="mariadb">
      <url-template name="default" template="jdbc:mariadb://address=\(protocol=tcp\)\(&lt;\)\(,host={host:host_ipv6:localhost},port={port::3306},user={user},password={password},{:identifier}={:identifier}&gt;\)[/{database}][\?&lt;&amp;,{:identifier}={:param}&gt;]" disable="true" />
      <url-template name="default" template="jdbc:mariadb://{host::localhost}?[:{port::3306}][/{database}?][\?&lt;&amp;,user={user},password={password},{:identifier}={:identifier}&gt;]" disable="true" />
      <artifact id="MariaDB Connector J" name="MariaDB Connector J" use="true" version="2.6.0" />
    </driver>
    <driver id="mysql.8" icon="providers/mariadb.svg">
      <artifact />
      <artifact id="MySQL ConnectorJ" name="MySQL ConnectorJ" use="true" version="8.0.21" />
    </driver>
  </component>
</application>